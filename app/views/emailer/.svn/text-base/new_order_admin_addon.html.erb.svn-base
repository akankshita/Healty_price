<% @file = File.open(Dir.pwd + "/public/mail_templates/new_order_admin_addon.html.erb") %><% @template = @file.read %><%	@template = @template.gsub("{order_number}", @order_number) %><%	@template = @template.gsub("{procedure}", @procedure) %><%	@template = @template.gsub("{doctor.name}", @doctor.name) %><%	@template = @template.gsub("{doctor.id}", @doctor.id.inspect) %><%	@template = @template.gsub("{customer_price}", number_to_currency(@order.customer_price,{:precision => 2})) %><%	@template = @template.gsub("{doctor_price}", number_to_currency(@order.doctor_price,{:precision => 2})) %><%	@template = @template.gsub("{billing_code}", @order.billing_code) %><%	@template = @template.gsub("{pin}", @order.pin) %><%	@template = @template.gsub("{created_at}", @order.str_date) %><%	@template = @template.gsub("{orderstatus}", @order.orderstatus) %><%	@template = @template.gsub("{doctor_payments_id}", (@order.doctor_payments_id==-1)?"None":@order.doctor_payments_id.inspect) %><%	@template = @template.gsub("{order_notes}", (@order.order_notes==nil)?"":@order.order_notes) %><%	@template = @template.gsub("{patient_name}", @patient_name) %><%	@template = @template.gsub("{order_name}", @order.name) %><%	@template = @template.gsub("{phone}", @order.phone) %><%	@template = @template.gsub("{email}", @order.email) %><%	@template = @template.gsub("{address}", @order.address2lines) %><%	@template = @template.gsub("{payment_method}", (@doctor.payment_method==nil)?"":@doctor.payment_method) %><%	@template = @template.gsub("{paypal_email}", (@doctor.paypal_email==nil)?"":@doctor.paypal_email) %><%	@template = @template.gsub("{payee_name}", (@doctor.payee_name==nil)?"":@doctor.payee_name) %><%	@template = @template.gsub("{payee_phone_number}", (@doctor.payee_phone_number==nil)?"":@doctor.payee_phone_number) %><%	@template = @template.gsub("{payee_email_address}", (@doctor.payee_email_address==nil)?"":@doctor.payee_email_address) %><%	@template = @template.gsub("{payee_address}", ((@doctor.payee_address1==nil)?"":@doctor.payee_address1+" ")+((@doctor.payee_address2==nil)?"":@doctor.payee_address2)) %><%	@template = @template.gsub("{payee_city}", (@doctor.payee_city==nil)?"":@doctor.payee_city) %><%	@template = @template.gsub("{payee_state}", (@doctor.payee_state==nil)?"":@doctor.payee_state) %><%	@template = @template.gsub("{payee_zip}", (@doctor.payee_zip==nil)?"":@doctor.payee_zip) %><%	@template = @template.gsub("{payee_tax_id_type}", (@doctor.payee_tax_id_type==nil)?"":@doctor.payee_tax_id_type) %><%	@template = @template.gsub("{payee_tax_id}", (@doctor.payee_tax_id==nil)?"":@doctor.payee_tax_id) %><%	@template = @template.gsub("{market_survey}", (@order.market_survey==nil)?"":@order.market_survey) %><%= @template %>