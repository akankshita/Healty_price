div.signup_header_text{
	text-align:left;
	float:left;
	margin-left:40px;
	font-family:Verdana, Arial, Helvetica, sans-serif;
}

#error{
	clear:both;
	float:left;
	/*padding:6px;*/
	width:600px;
	border:1px solid #FF0000;
	background-color:#FFCCCC;
	margin-top:8px;
	margin-bottom:12px;
