	margin-left:40px;
}

#error ul{
	margin-top:8px;
	margin-bottom:8px;
	margin-left:21px;
}

input[type="submit"]{
	width:206px;
	height:26px;
	margin-top:0px;
	margin-bottom:0px;
}

/* ------------------------------------------------------------------------- */

div.file_upload_wrapper{
	width: 300px;
	height: 100px;
	float: left;
	position: relative;
	visibility: hidden;
	display:none;
}

input.fileinput{
	/*position: absolute;
	width:75px; height:26px; top:0; left:0;
	visibility: hidden;
	display:none;*/
}

span.browse{
	position: absolute;
	width: 79px;
	height: 24px;
	top: 0;
	left: 0;
	background: url('/images/provider_signup/browse.jpg') left top no-repeat;
	cursor:pointer;
}

span.filename{
	position: absolute;
	width: 175px;
	height: 22px;
	left: 75px;
	text-align: left;
	background-color:white;
	border:1px solid #666666;
	/*background: url(filename.png) left top no-repeat;*/
}

