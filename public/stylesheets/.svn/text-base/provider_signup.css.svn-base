/* blue box */
div.blue-box {
  border: 4px #888888 solid;
  width:400px;
  position:relative;
  top:-50px;
  left:240px;
}

div.blue-box-header {
  border-bottom: 1px #9c9c9c solid;
  background-color:#dceefa;
  color:#444444;
}

div.blue-box-header h3 {
  color:#444444;
  font-size:16pt;
  margin-bottom: 0px;
  padding:0.5em;
  margin-left:0px;
}

div.blue-box-body {
  padding:10px;
  background-color:white;
}

