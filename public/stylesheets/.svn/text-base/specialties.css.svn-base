div#specialties {
  overflow: hidden;
  margin-top: 10px;
}

div#specialties li {
  list-style: none;
  padding: 0px;
  margin: 5px;
  float:left;
  width: 45%;
  font-size:11pt;
}

div#specialties ul {
  padding: 0px;
  margin: 0px;
}

div#other_search {
  border: 1px #bbbbbb solid;
  padding: 4px;
  background-color: white;
  float: right;
}

div#other_search a {
  padding-left: 5px;
  padding-right: 5px;
}
